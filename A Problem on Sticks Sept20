#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007
int main()
{
            int test;   cin>>test;
            while(test--){
                 int n;
                cin>>n;
                unordered_map<int,int> mp;
                for(int i=0;i<n;i++){
                    int x; cin>>x;
                    if(x != 0){
                        mp[x]++;
                    }
                }
                int cnt=0;
                for(auto it: mp){
                    cnt++;

                }
                cout<<cnt<<endl;
            }
}


