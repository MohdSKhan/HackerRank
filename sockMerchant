
#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007
int main()
{

            int n,pairs=0;
            cin>>n;
            int arr[n];
            for(int i=0;i<n;i++){
                cin>>arr[i];
            }
            sort(arr,arr+n);
            for(int i=1;i<n;i++){
                if(arr[i] == arr[i-1]){
                    pairs++;
                    i++;
                }
            }
            cout<<pairs<<endl;





}


